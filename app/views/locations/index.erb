<h2>All Locations(by Type)</h2>

<%@types.each do |type|%>
<h6><%=type.name%></h6>
  <%@locations.each do |place|%>
    <%if place.type == type.id%>
    <p>place.name</p>
    <p>place.address</p
    <%place.users.each do |user|%>
    <a href="users/<%=user.id%>"><%=user.name%></a>
    <%end%>
    <%end%>
    <%end%>
    <%end%>
    
<%if !!session[:email]%>
<%@user = User.find_by(email: session[:email])%>

<a href="/users/<%=@user.id%>/">Return to Profile</a>
<a href="/guides">Explore all Guides</a>
<%else%>
<a href="/guides">Explore all Guides</a>
<%end%>

  